      "@medusajs/ui": "^4.0.21",
      "@medusajs/icons": "^2.10.1",
      "@mikro-orm/cli": "6.4.3",
      "@mikro-orm/core": "6.4.3",
      "@mikro-orm/knex": "6.4.3",
      "@mikro-orm/migrations": "6.4.3",
      "@mikro-orm/postgresql": "6.4.3",
      "awilix": "^8.0.1",
      "pg": "^8.13.0"
   },
   "dependencies": {
      "pdfkit": "^0.17.2",
      "postmark": "^4.0.5"
   },
   "engines": {
      "node": ">=20"
   }
